{
  "root": "out",
  "clean_urls": true,
  "routes": {
    "/**": "index.html"
  },
  "headers": {
    "/**.js": {
      "Cache-Control": "public, max-age=31536000, immutable"
    },
    "/**.css": {
      "Cache-Control": "public, max-age=31536000, immutable"
    },
    "/**.png": {
      "Cache-Control": "public, max-age=31536000, immutable"
    },
    "/**.jpg": {
      "Cache-Control": "public, max-age=31536000, immutable"
    },
    "/**.svg": {
      "Cache-Control": "public, max-age=31536000, immutable"
    }
  }
}
